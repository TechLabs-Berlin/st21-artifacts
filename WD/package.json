{
  "name": "artifacts",
  "license": "MIT",
  "scripts": {
    "deploy-to-firebase": "yarn build && npx firebase deploy",
    "serve": "live-server dist/",
    "build": "yarn copy-assets &&  webpack --mode production",
    "copy-assets": "rm -rf dist && mkdir dist && cp -r public/* dist",
    "dev-server": "yarn copy-assets && webpack serve --mode development --open",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix"
  },
  "dependencies": {
    "emailjs-com": "^3.1.0",
    "firebase": "4.2.0",
    "history": "4.7.2",
    "normalize.css": "7.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "4.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "css-loader": "^5.2.6",
    "eslint": "^7.29.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-react": "^7.24.0",
    "live-server": "^1.2.1",
    "node-sass": "^6.0.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.0.0",
    "webpack": "^5.40.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  }
}
